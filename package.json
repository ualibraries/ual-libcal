{
  "name": "ual-libcal",
  "version": "1.0.0",
  "description": "The University of Arizona Libraries LibCal customizations",
  "main": "dist/bundle.js",
  "scripts": {
    "build:dev": "node_modules/webpack/bin/webpack.js -w --verbose",
    "build:prod": "npm test && node_modules/webpack/bin/webpack.js -p",
    "deploy:dev": "node_modules/shipit-cli/bin/shipit dev deploy",
    "deploy:prod": "npm test && node_modules/shipit-cli/bin/shipit prod deploy",
    "serve": "node ./static-server.js",
    "test": "node_modules/standard/bin/cmd.js && node_modules/stylelint/bin/stylelint.js \"app/styles/*.css\" && ava --verbose"
  },
  "dependencies": {
    "@webcomponents/webcomponentsjs": "github:webcomponents/webcomponentsjs",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
    "babel-loader": "^7.1.1",
    "babel-preset-es2015": "^6.24.1",
    "css-loader": "^0.28.4",
    "file-loader": "^0.11.2",
    "handlebars": "^4.0.10",
    "html-loader": "^0.4.5",
    "lodash": "^4.17.4",
    "postcss-cssnext": "^2.11.0",
    "postcss-import": "^10.0.0",
    "postcss-loader": "^2.0.6",
    "postcss-reporter": "^4.0.0",
    "style-loader": "^0.18.2",
    "stylelint": "^7.12.0",
    "url-loader": "^0.5.9",
    "webpack": "^3.0.0"
  },
  "devDependencies": {
    "ava": "^0.20.0",
    "browser-env": "^3.0.2",
    "jsdom": "^11.1.0",
    "localtunnel": "^1.8.3",
    "moment": "^2.18.1",
    "node-static": "^0.7.9",
    "shipit-cli": "^3.0.0",
    "shipit-deploy": "^2.5.1",
    "standard": "^10.0.2",
    "yargs": "^8.0.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ualibraries/ual-libcal.git"
  },
  "author": "Christopher Green",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ualibraries/ual-libcal/issues"
  },
  "homepage": "https://github.com/ualibraries/ual-libcal#readme",
  "ava": {
    "require": [
      "./test/helpers/setup-browser-env.js"
    ]
  }
}
